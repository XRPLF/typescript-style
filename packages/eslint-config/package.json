    "eslint": "^7.5.0",
    "eslint-find-rules": "^3.6.1",
    "eslint-plugin-array-func": "^3.1.6",
    "eslint-plugin-consistent-default-export-name": "^0.0.13",
    "eslint-plugin-eslint-comments": "^3.2.0",
