{
  "name": "@patelraj96/eslint-config",
  "version": "1.10.6",
  "publishConfig": {
    "access": "public"
  },
  "description": "The XRPL Foundation's base TypeScript ESLint config, following our styleguide",
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "styleguide"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/XRPLF/typescript-style.git"
  },
  "license": "ISC",
  "main": "index.js",
  "scripts": {
    "check-prettier": "npx eslint-config-prettier index.js",
    "eslint-find-option-rules": "eslint-find-rules [option] <file> [flag]",
    "lint": "npm run check-prettier && eslint --report-unused-disable-directives .",
    "plugin": "eslint-find-rules --plugin",
    "unused": "eslint-find-rules --unused"
  },
  "dependencies": {
    "confusing-browser-globals": "^1.0.11",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.40.0",
    "eslint-plugin-array-func": "^4.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^52.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-tsdoc": "^0.2.16",
    "prettier": "^2.7.1",
    "typescript": ">=4.7.4"
  },
  "prettier": "@patelraj96/prettier-config",
  "gitHead": "9cf6f8a06558c73f027fea977898510ab20c5283",
  "engines": {
    "node": ">=20.11.0",
    "npm": ">= 8.0.0"
  }
}
